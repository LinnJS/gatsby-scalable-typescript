{
  "name": "hyve-email",
  "description": "Hyve email security",
  "version": "0.1.0",
  "author": "Justin Linn <linnjsconsulting@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/LinnJS/gatsby-scalable-typescript"
  },
  "keywords": [
    "gatsby",
    "storybook",
    "styled-components",
    "normalize-css",
    "eslint",
    "netlify",
    "scalable",
    "esm"
  ],
  "scripts": {
    "start": "gatsby develop -H 0.0.0.0",
    "open": "gatsby develop -H 0.0.0.0 -o",
    "storybook": "start-storybook -p 9000 -c .storybook",
    "build": "gatsby build",
    "build:storybook": "build-storybook -c .storybook -o storybook",
    "build:all": "gatsby build && build-storybook -c .storybook -o storybook",
    "serve": "gatsby serve -o -p 8080",
    "clean": "gatsby clean",
    "lint": "eslint src -c .eslintrc.js --ext js,jsx,ts,tsx",
    "lint:fix": "eslint src -c .eslintrc.js --fix --ext js,jsx,ts,tsx",
    "prepare": "husky install",
    "type-check": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint:fix && yarn type-check"
    }
  },
  "dependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/react-helmet": "^6.1.0",
    "@types/styled-components": "^5.1.7",
    "gatsby": "^3.7.2",
    "gatsby-image": "^3.7.1",
    "gatsby-plugin-manifest": "^3.7.1",
    "gatsby-plugin-offline": "^4.7.1",
    "gatsby-plugin-react-helmet": "^4.7.1",
    "gatsby-plugin-root-import": "^2.0.5",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-styled-components": "^4.7.1",
    "gatsby-plugin-typescript": "^3.7.1",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "styled-components": "^5.2.1",
    "styled-normalize": "^8.0.7",
    "styled-reset-advanced": "^1.0.1"
  },
  "devDependencies": {
    "@babel/helper-builder-react-jsx": "^7.10.4",
    "@storybook/addon-actions": "^6.1.14",
    "@storybook/addon-links": "^6.1.14",
    "@storybook/addon-notes": "^5.3.21",
    "@storybook/addons": "^6.1.14",
    "@storybook/react": "^6.1.14",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "babel-plugin-styled-components": "^1.12.0",
    "eslint": "^7.18.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-import-resolver-webpack": "^0.13.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-standard": "^5.0.0",
    "esm": "^3.2.25",
    "husky": "^6.0.0",
    "prettier": "^2.2.1",
    "typescript": "^4.1.3"
  }
}
